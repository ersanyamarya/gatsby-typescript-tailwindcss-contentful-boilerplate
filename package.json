{
  "name": "gatsby-starter-typescript-contentful",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby with Typescript and Contentful",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "lint-fix": "eslint --ext ts --fix ./src",
    "lint": "eslint --ext ts ./src",
    "clean": "gatsby clean && rm -rf public",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "lint-staged": {
    "*.ts": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@contentful/gatsby-transformer-contentful-richtext": "^13.1.0",
    "dotenv": "^8.2.0",
    "gatsby": "^2.24.60",
    "gatsby-image": "^2.4.18",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-sharp": "^2.6.36",
    "gatsby-plugin-ts-config": "^1.0.0",
    "gatsby-source-contentful": "^2.3.46",
    "gatsby-source-filesystem": "^2.3.30",
    "gatsby-transformer-sharp": "^2.5.15",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@graphql-codegen/cli": "^1.17.8",
    "@graphql-codegen/introspection": "^1.17.8",
    "@graphql-codegen/typescript": "^1.17.9",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@graphql-codegen/typescript-react-apollo": "^2.0.6",
    "@types/node": "^14.10.2",
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-standard": "^4.0.1",
    "gatsby-plugin-emotion": "^4.3.11",
    "gatsby-plugin-eslint": "^2.0.8",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "twin.macro": "^1.9.0",
    "typescript": "^4.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ersanyamarya/gatsby-typescript-tailwindcss-contentful-boilerplate"
  },
  "bugs": {
    "url": "https://github.com/ersanyamarya/gatsby-typescript-tailwindcss-contentful-boilerplate/issues"
  },
  "homepage": "https://github.com/ersanyamarya/gatsby-typescript-tailwindcss-contentful-boilerplate#readme",
  "author": {
    "name": "Sanyam Arya",
    "email": "er.sanyam.arya@gmail.com",
    "url": "https://github.com/ersanyamarya"
  }
}
